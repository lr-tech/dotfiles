def_key "l"
  next_column

def_key "h"
  previous_column

def_key "k"
  scroll_up

def_key "j"
  scroll_down

def_key "ctrl-f"
  page_down

def_key "ctrl-b"
  page_up

def_key "g"
  move_home

def_key "G"
  move_end

def_key "J"
  move_sort_order_down

def_key "K"
  move_sort_order_up

def_key "d"
  delete_playlist_items

def_key "d"
  delete_browser_items

def_key "d"
  delete_stored_playlist

def_key "n"
  next_found_item

def_key "N"
  previous_found_item

def_key "+"
  show_clock

def_key "="
  volume_up

def_key "-"
  volume_down

def_key "L"
  show_lyrics

def_key "ctrl-l"
  toggle_lyrics_fetcher

